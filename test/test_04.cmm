int fibonacci(int n) {
    if (n <= 0) return 0;
    else if (n <= 2) return 1;
    
    int a, b, res, i;
    a = b = 1;
    res = 0;
    i = 2;
    while (i < n) {
        res = a + b;
        a = b;
        b = res;
        i = i + 1;
    }
    return res;
}

int main() {
    int n, res;
    n = input();
    res = fibonacci(n);
    output(res);
    return 0;
}